    },
    "CommentTable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "ModuleTable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "proyectoAuthPostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "UserTable",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "WebcsFunction": {
      "build": true,
